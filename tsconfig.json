{
  "compilerOptions": {
    // "target": "ES5",
    "target": "es2015",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ],
    "strictNullChecks": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "pages/api/auth/[...nextauth].ts",
    ".trash/_app copy.tsx",
    "components/3d/Hippo_001.jsx",
    "**/*.mp3", //わからん
    "**/*.d.ts",
    "pages/api/typing_.js",
    "pages/api/typing.ts",
    ".next/types/**/*.ts",
    "libs/.blog-post.ts",
    "pages/app/typing.jsx"
  ],
  "exclude": ["node_modules", "public/files"]
}
